{
  "manifest": "data/wlasl_preprocessed/manifest_nslt100.csv",
  "clip_len": 32,
  "frame_stride": 2,
  "batch_size": 8,
  "num_workers": 4,
  "use_weighted_sampler": true,
  "normalize": "kinetics",
  "backbone": "r3d18_k400",
  "dropout": 0.2,
  "label_smoothing": 0.0,
  "epochs": 25,
  "warmup_epochs": 2,
  "lr_head_stage1": 0.001,
  "lr_back_stage1": 0.0,
  "lr_head_stage2": 0.0001,
  "lr_back_stage2": 1e-05,
  "weight_decay": 1e-05,
  "cosine_eta_min": 1e-05,
  "amp": true,
  "compile": true,
  "freeze_backbone": true,
  "unfreeze_at_epoch": 5,
  "unfreeze_scope": "layer4",
  "checkpoints_dir": "checkpoints",
  "runs_dir": "runs",
  "seed": 42
}